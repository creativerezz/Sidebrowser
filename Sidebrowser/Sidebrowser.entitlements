<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Required for WKWebView to work properly -->
	<key>com.apple.security.app-sandbox</key>
	<true/>
	
	<!-- Allow outgoing network connections -->
	<key>com.apple.security.network.client</key>
	<true/>
	
	<!-- Allow WKWebView to render properly with GPU acceleration -->
	<key>com.apple.security.cs.allow-jit</key>
	<true/>
	
	<!-- Allow access to temporary files needed by WebKit -->
	<key>com.apple.security.temporary-exception.files.absolute-path.read-write</key>
	<array>
		<string>/private/tmp/</string>
	</array>
	
	<!-- Disable library validation for development -->
	<key>com.apple.security.cs.disable-library-validation</key>
	<true/>
</dict>
</plist>
